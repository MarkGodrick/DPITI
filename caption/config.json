{
    "captioner":{
        "openai":{
            "openai_run":{
                "model":"gpt-4o-mini",
                "max_completion_tokens":100,
                "temperature":1.0
            },
            "batch_size":1024,
            "max_workers":16,
            "progress_bar":true
        },
        "gemini":{
            "model":"gemini-2.0-flash",
            "gemini_run":{
                "max_output_tokens":100,
                "temperature":0.1
            },
            "prompts":"Please describe the image with detail",
            "batch_size":16,
            "max_workers":4,
            "progress_bar":true
        },
        "huggingface":{
            "hf_model":{
                "model":"Salesforce/blip-image-captioning-large",
                "device":"cuda"
            },
            "hf_run":{
                "batch_size":16,
                "num_workers":8,
                "max_new_tokens":100
            }
        }
    },
    "dataset_path":"dataset/lsun",
    "lsun_class": "bedroom_train"
}